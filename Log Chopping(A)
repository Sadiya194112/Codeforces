#include<bits/stdc++.h>
using namespace std;
int main(){
    int tc;
    cin>>tc;
    while(tc--){
        int n;
        cin>>n;
        int a[n+3],i,e=0,m=0;
        for(i=0; i<n; i++){
            cin>>a[i];

        }
        for(i=0; i<n; i++){
            if(a[i]%2==0)   e++;
        }
        if(e%2==0) cout<<"maomao90\n";
        else if(e%2!=0)    cout<<"errorgorn\n";
    }
    return 0;
}
