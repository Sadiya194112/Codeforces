#include<bits/stdc++.h>
using namespace std;
void ac(){
    int a,b,c,d,cnt=0;
    cin>>a>>b>>c>>d;
    if(b>a) cnt++;
    if(c>a) cnt++;
    if(d>a) cnt++;
    cout<<cnt<<"\n";
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(0);     cout.tie(0);
    int tc;     cin>>tc;
    while(tc--){
       ac();
    }
    return 0;
}
