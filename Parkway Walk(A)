@Sadiya

#include<bits/stdc++.h>
using namespace std;
int main(){
    int tc; cin>>tc;
    while(tc--){
        int n,m,sum=0;  cin>>n>>m;
        int a[n+3];
        for(int i=1; i<=n; i++){
            cin>>a[i];
            sum+=a[i];
        }
        if(m>sum)   cout<<"0\n";
        else    cout<<sum-m<<"\n";
    }
}
