#include<bits/stdc++.h>
#define ll long long
#define rep(i, c)   for(int i=0; i<(int)c; i++)
using namespace std;
void Solve(){
    vector<int> b;
    int x;
    for(int i=0; i<7; i++){
        cin>>x;
        b.push_back(x);
    }
    ll req=b[6];
    ll sum1=0;
    ll sum2=0;
    sum1 = b[0]+b[1]+b[2];
    sum2 = b[0]+b[1]+b[3];
    if(req==sum1)   cout<<b[0]<<" "<<b[1]<<" "<<b[2]<<"\n";
    else    cout<<b[0]<<" "<<b[1]<<" "<<b[3]<<"\n";
}
int main(){
    int tc;     cin>>tc;
    while(tc--){
        Solve();
    }
    return 0;
}
